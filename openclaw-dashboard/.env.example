# OpenClaw live integration toggle (default uses local mock data)
REACT_APP_OPENCLAW_LIVE_DATA=false

# REST API base URL (example)
REACT_APP_OPENCLAW_API_BASE_URL=http://localhost:8787

# Optional explicit WebSocket URL (if omitted, inferred from API base URL)
REACT_APP_OPENCLAW_WS_URL=ws://localhost:8787

# API timeout in milliseconds
REACT_APP_OPENCLAW_API_TIMEOUT_MS=120000
